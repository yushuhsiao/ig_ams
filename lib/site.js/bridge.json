{
  "output": "Resources/.generated",
  "cleanOutputFolderBeforeBuild": true,
  "outputFormatting": "Formatted",
  "outputBy": "ClassPath",
  "generateDocumentation": "None",
  "logging": {
    "level": "None"
  },
  "html": {
    "disabled": true
  },
  "loader": {
    "manualLoading": true,
    "skipManualVariables": true
  },
  "assembly": {
    //"disableInitAssembly": true,
    "enableReservedNamespaces": true
  },
  "fileName": "generated.js",
  "combineScripts": false,
  "resources": [
    {
      "name": "bridge.js",
      "extract": false
    },
    {
      "name": "bridge.meta.js",
      "extract": false
    },
    {
      "name": "bridge.console.js",
      "extract": false
    },
    {
      "name": "newtonsoft.json.js",
      "extract": false
    },
    {
      // File paths relative to project root
      "name": "site.js",
      //"header": "Resources/.auxiliary/Header.js",
      //"remark": "\n\n// @source @{name}\n",
      "files": [
        //"Resources/.auxiliary/AssemblyBegin.js",
        "Resources/.generated/generated.js",
        "Resources/.generated/generated.meta.js"
        //"Resources/Manual/JsonConvert.js",
        //"Resources/.auxiliary/AssemblyEnd.js"
      ]
    }
  ]
}