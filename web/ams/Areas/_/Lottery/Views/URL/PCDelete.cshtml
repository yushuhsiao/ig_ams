@model GameControl.Models.Pc_Keno_Sample

@{
    ViewBag.Title = "PCDelete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/table.css" rel="stylesheet" />
<h2>盤口刪除</h2>

<div class="services">
    <div class="CSSTable">
        <table>
            <tr>
                <td style="width:10%">
                    項目
                </td>
                <td style="width:80%">
                    屬性
                </td>
            </tr>
            <tr>
                <td>
                    盤口
                </td>
                <td>
                    @Html.DisplayFor(model => model.Dish)
                </td>
            </tr>
        </table>
    </div>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="刪除" class="btn btn-info btn-lg" />
            <input class="btn btn-warning btn-lg" type="button" value="回前頁" onclick="location.href='@Url.Action("PassAccountIndex", "URL")'" />
        </div>
    }
</div>